{
  "api": "indices.get_mapping",
  "file": "/test/free/search.vectors/60_dense_vector_dynamic_mapping.yml",
  "name": "We respect values set in dynamic_templates for floats and dense vectors",
  "origin": "yaml",
  "request": {
    "args": {
      "index": "my-dynamic-template-index"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "my-dynamic-template-index": {
        "mappings": {
          "dynamic_templates": [
            {
              "my_vector_template": {
                "mapping": {
                  "dims": 5,
                  "index": true,
                  "similarity": "cosine",
                  "type": "dense_vector"
                },
                "match": "*dense_vector*"
              }
            },
            {
              "my_float_template": {
                "mapping": {
                  "type": "float"
                },
                "match": "*float*"
              }
            }
          ],
          "properties": {
            "my_dense_vector_field": {
              "dims": 5,
              "index": true,
              "index_options": {
                "ef_construction": 100,
                "m": 16,
                "type": "int8_hnsw"
              },
              "similarity": "cosine",
              "type": "dense_vector"
            },
            "my_dynamic_field": {
              "dims": 128,
              "index": true,
              "index_options": {
                "ef_construction": 100,
                "m": 16,
                "type": "int8_hnsw"
              },
              "similarity": "cosine",
              "type": "dense_vector"
            },
            "my_float_field": {
              "type": "float"
            },
            "my_small_float_field": {
              "type": "float"
            }
          }
        }
      }
    },
    "statusCode": 200
  }
}