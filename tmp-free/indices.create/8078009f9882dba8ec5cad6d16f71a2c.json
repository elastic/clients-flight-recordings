{
  "api": "indices.create",
  "file": "/test/free/get/100_synthetic_source.yml",
  "name": "fields with ignore_malformed",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "properties": {
            "bool_field": {
              "ignore_malformed": true,
              "type": "boolean"
            },
            "double_field": {
              "ignore_malformed": true,
              "type": "double"
            },
            "int_field": {
              "ignore_malformed": true,
              "type": "integer"
            },
            "ip": {
              "ignore_malformed": true,
              "type": "ip"
            }
          }
        },
        "settings": {
          "index": {
            "mapping": {
              "source": {
                "mode": "synthetic"
              }
            }
          }
        }
      },
      "index": "test"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "71",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "test",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}