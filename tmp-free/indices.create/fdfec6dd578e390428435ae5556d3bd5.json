{
  "api": "indices.create",
  "file": "/test/free/search/200_index_phrase_search.yml",
  "name": "search with indexed phrases",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "properties": {
            "text": {
              "index_phrases": true,
              "type": "text"
            }
          }
        }
      },
      "index": "test"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "71",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "test",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}