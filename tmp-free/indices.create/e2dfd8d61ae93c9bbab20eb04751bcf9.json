{
  "api": "indices.create",
  "file": "/test/free/search/150_rewrite_on_coordinator.yml",
  "name": "Ensure that we fetch the document only once",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "properties": {
            "followers": {
              "type": "keyword"
            }
          }
        },
        "settings": {
          "number_of_shards": 1
        }
      },
      "index": "lookup_index"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "75",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "lookup_index",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}