{
  "api": "indices.create",
  "file": "/test/free/bulk/11_dynamic_templates.yml",
  "name": "Dynamic templates",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "dynamic_templates": [
            {
              "location": {
                "mapping": {
                  "type": "geo_point"
                }
              }
            },
            {
              "my_location": {
                "mapping": {
                  "type": "geo_point"
                },
                "match": "my*"
              }
            },
            {
              "string": {
                "mapping": {
                  "type": "keyword"
                }
              }
            }
          ]
        },
        "settings": {
          "number_of_shards": 1
        }
      },
      "index": "test_index"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "73",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "test_index",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}