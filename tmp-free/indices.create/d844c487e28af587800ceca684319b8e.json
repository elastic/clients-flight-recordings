{
  "api": "indices.create",
  "file": "/test/free/search/400_synthetic_source.yml",
  "name": "stored keyword with ignore_above",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "properties": {
            "kwd": {
              "doc_values": false,
              "ignore_above": 10,
              "store": true,
              "type": "keyword"
            }
          }
        },
        "settings": {
          "index": {
            "mapping": {
              "source": {
                "mode": "synthetic"
              }
            }
          }
        }
      },
      "index": "test"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "71",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "test",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}