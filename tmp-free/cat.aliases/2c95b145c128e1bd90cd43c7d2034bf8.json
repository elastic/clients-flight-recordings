{
  "api": "cat.aliases",
  "file": "/test/free/cat.aliases/10_basic.yml",
  "name": "Multiple alias names",
  "origin": "yaml",
  "request": {
    "args": {
      "format": "json",
      "h": [
        "alias",
        "index"
      ],
      "name": [
        "foo",
        "bar"
      ],
      "s": [
        "index"
      ],
      "v": true
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": [
      {
        "alias": "foo",
        "index": "test"
      },
      {
        "alias": "bar",
        "index": "test2"
      }
    ],
    "statusCode": 200
  }
}