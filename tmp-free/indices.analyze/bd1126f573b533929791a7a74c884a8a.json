{
  "api": "indices.analyze",
  "file": "/test/free/analysis-common/40_token_filters.yml",
  "name": "length",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "filter": [
          "my_length"
        ],
        "text": "foo bar foobar",
        "tokenizer": "whitespace"
      },
      "index": "test"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "103",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "tokens": [
        {
          "end_offset": 14,
          "position": 2,
          "start_offset": 8,
          "token": "foobar",
          "type": "word"
        }
      ]
    },
    "statusCode": 200
  }
}