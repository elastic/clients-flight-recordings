{
  "api": "indices.get_index_template",
  "file": "/test/free/indices.get_index_template/10_basic.yml",
  "name": "Add data stream lifecycle",
  "origin": "yaml",
  "request": {
    "args": {
      "name": "test-lifecycle"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "250",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "index_templates": [
        {
          "index_template": {
            "composed_of": [
            ],
            "data_stream": {
              "allow_custom_routing": false,
              "failure_store": false,
              "hidden": false
            },
            "index_patterns": [
              "data-stream-with-lifecycle-*"
            ],
            "template": {
              "lifecycle": {
                "data_retention": "30d",
                "enabled": true
              },
              "mappings": {
                "properties": {
                  "field": {
                    "type": "keyword"
                  }
                }
              },
              "settings": {
                "index": {
                  "number_of_shards": "1"
                }
              }
            }
          },
          "name": "test-lifecycle"
        }
      ]
    },
    "statusCode": 200
  }
}