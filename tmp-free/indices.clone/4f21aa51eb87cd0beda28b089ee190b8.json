{
  "api": "indices.clone",
  "file": "/test/free/indices.clone/10_basic.yml",
  "name": "Clone index via API",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "settings": {
          "index": {
            "number_of_shards": 2
          }
        }
      },
      "index": "source",
      "master_timeout": "10s",
      "target": "target"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "73",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "target",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}