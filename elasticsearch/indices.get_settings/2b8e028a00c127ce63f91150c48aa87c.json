{
  "api": "indices.get_settings",
  "file": "/test/free/indices.get_settings/10_basic.yml",
  "name": "Get /{index}/_settings/{name*}",
  "origin": "yaml",
  "request": {
    "args": {
      "index": "test_1",
      "name": "index.number_of_s*"
    }
  },
  "response": {
    "headers": {
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "test_1": {
        "settings": {
          "index": {
            "number_of_shards": "5"
          }
        }
      }
    },
    "statusCode": 200
  }
}