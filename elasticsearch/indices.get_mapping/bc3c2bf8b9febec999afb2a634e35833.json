{
  "api": "indices.get_mapping",
  "file": "/test/platinum/downsample-with-security/10_basic.yml",
  "name": "Downsample index",
  "origin": "yaml",
  "request": {
    "args": {
      "index": "test-downsample"
    }
  },
  "response": {
    "headers": {
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "test-downsample": {
        "mappings": {
          "_data_stream_timestamp": {
            "enabled": true
          },
          "properties": {
            "@timestamp": {
              "meta": {
                "fixed_interval": "1h",
                "time_zone": "UTC"
              },
              "type": "date"
            },
            "k8s": {
              "properties": {
                "pod": {
                  "properties": {
                    "created_at": {
                      "type": "date_nanos"
                    },
                    "ip": {
                      "type": "ip"
                    },
                    "multi-counter": {
                      "time_series_metric": "counter",
                      "type": "long"
                    },
                    "multi-gauge": {
                      "default_metric": "max",
                      "metrics": [
                        "min",
                        "max",
                        "sum",
                        "value_count"
                      ],
                      "time_series_metric": "gauge",
                      "type": "aggregate_metric_double"
                    },
                    "name": {
                      "type": "keyword"
                    },
                    "network": {
                      "properties": {
                        "rx": {
                          "default_metric": "max",
                          "metrics": [
                            "min",
                            "max",
                            "sum",
                            "value_count"
                          ],
                          "time_series_metric": "gauge",
                          "type": "aggregate_metric_double"
                        },
                        "tx": {
                          "default_metric": "max",
                          "metrics": [
                            "min",
                            "max",
                            "sum",
                            "value_count"
                          ],
                          "time_series_metric": "gauge",
                          "type": "aggregate_metric_double"
                        }
                      }
                    },
                    "number_of_containers": {
                      "type": "integer"
                    },
                    "running": {
                      "type": "boolean"
                    },
                    "tags": {
                      "type": "keyword"
                    },
                    "uid": {
                      "time_series_dimension": true,
                      "type": "keyword"
                    },
                    "values": {
                      "type": "integer"
                    }
                  }
                }
              }
            },
            "metricset": {
              "time_series_dimension": true,
              "type": "keyword"
            }
          }
        }
      }
    },
    "statusCode": 200
  }
}