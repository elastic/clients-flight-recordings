{
  "api": "indices.get_mapping",
  "file": "/test/platinum/downsample/10_basic.yml",
  "name": "Downsample a downsampled index",
  "origin": "yaml",
  "request": {
    "args": {
      "index": "test-downsample-2"
    }
  },
  "response": {
    "headers": {
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "test-downsample-2": {
        "mappings": {
          "_data_stream_timestamp": {
            "enabled": true
          },
          "properties": {
            "@timestamp": {
              "meta": {
                "fixed_interval": "2h",
                "time_zone": "UTC"
              },
              "type": "date"
            },
            "k8s.pod.created_at": {
              "type": "date_nanos"
            },
            "k8s.pod.ip": {
              "type": "ip"
            },
            "k8s.pod.multi-counter": {
              "time_series_metric": "counter",
              "type": "long"
            },
            "k8s.pod.multi-gauge": {
              "default_metric": "max",
              "metrics": [
                "min",
                "max",
                "sum",
                "value_count"
              ],
              "time_series_metric": "gauge",
              "type": "aggregate_metric_double"
            },
            "k8s.pod.name": {
              "type": "keyword"
            },
            "k8s.pod.network.rx": {
              "default_metric": "max",
              "metrics": [
                "min",
                "max",
                "sum",
                "value_count"
              ],
              "time_series_metric": "gauge",
              "type": "aggregate_metric_double"
            },
            "k8s.pod.network.tx": {
              "default_metric": "max",
              "metrics": [
                "min",
                "max",
                "sum",
                "value_count"
              ],
              "time_series_metric": "gauge",
              "type": "aggregate_metric_double"
            },
            "k8s.pod.number_of_containers": {
              "type": "integer"
            },
            "k8s.pod.running": {
              "type": "boolean"
            },
            "k8s.pod.scaled-counter": {
              "scaling_factor": 100,
              "time_series_metric": "counter",
              "type": "scaled_float"
            },
            "k8s.pod.scaled-gauge": {
              "default_metric": "max",
              "metrics": [
                "min",
                "max",
                "sum",
                "value_count"
              ],
              "time_series_metric": "gauge",
              "type": "aggregate_metric_double"
            },
            "k8s.pod.tags": {
              "type": "keyword"
            },
            "k8s.pod.uid": {
              "time_series_dimension": true,
              "type": "keyword"
            },
            "k8s.pod.values": {
              "type": "integer"
            },
            "metricset": {
              "time_series_dimension": true,
              "type": "keyword"
            }
          },
          "subobjects": false
        }
      }
    },
    "statusCode": 200
  }
}