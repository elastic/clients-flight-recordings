{
  "api": "indices.get_mapping",
  "file": "/test/platinum/exponential_histogram/50_time_series_metric.yml",
  "name": "Create time series metric with exponential histogram",
  "origin": "yaml",
  "request": {
    "args": {
      "index": "test_exponential_histogram_metric"
    }
  },
  "response": {
    "headers": {
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "test_exponential_histogram_metric": {
        "mappings": {
          "properties": {
            "my_metric": {
              "time_series_metric": "histogram",
              "type": "exponential_histogram"
            }
          }
        }
      }
    },
    "statusCode": 200
  }
}