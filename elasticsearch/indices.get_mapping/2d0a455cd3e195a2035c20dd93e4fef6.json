{
  "api": "indices.get_mapping",
  "file": "/test/platinum/exponential_histogram/50_time_series_metric.yml",
  "name": "Merging templates with time series metric",
  "origin": "yaml",
  "request": {
    "args": {
      "index": "my-templated-index"
    }
  },
  "response": {
    "headers": {
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "my-templated-index": {
        "mappings": {
          "properties": {
            "my_counter": {
              "time_series_metric": "counter",
              "type": "long"
            },
            "my_histogram": {
              "time_series_metric": "histogram",
              "type": "exponential_histogram"
            }
          }
        }
      }
    },
    "statusCode": 200
  }
}