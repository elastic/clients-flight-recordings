{
  "api": "indices.get_alias",
  "file": "/test/free/indices.put_alias/10_basic.yml",
  "name": "Can set is_write_index",
  "origin": "yaml",
  "request": {
    "args": {
      "index": "test_index",
      "name": "test_alias"
    }
  },
  "response": {
    "headers": {
      "content-length": "65",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "test_index": {
        "aliases": {
          "test_alias": {
            "is_write_index": true
          }
        }
      }
    },
    "statusCode": 200
  }
}