{
  "api": "search",
  "file": "/test/free/search.vectors/110_knn_query_with_filter.yml",
  "name": "PRE_FILTER: pre-filter across multiple internal filters",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "fields": [
          "my_name"
        ],
        "query": {
          "knn": {
            "field": "my_vector",
            "filter": [
              {
                "term": {
                  "my_name": "v1"
                }
              },
              {
                "term": {
                  "my_name": "v2"
                }
              }
            ],
            "num_candidates": 5,
            "query_vector": [
              1,
              1,
              1,
              1
            ]
          }
        },
        "size": 3
      },
      "index": "my_index",
      "typed_keys": true
    }
  },
  "response": {
    "headers": {
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "_shards": {
        "failed": 0,
        "skipped": 0,
        "successful": 1,
        "total": 1
      },
      "hits": {
        "hits": [
        ],
        "max_score": null,
        "total": {
          "relation": "eq",
          "value": 0
        }
      },
      "timed_out": false,
      "took": 2
    },
    "statusCode": 200
  }
}