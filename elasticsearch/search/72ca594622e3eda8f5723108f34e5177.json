{
  "api": "search",
  "file": "/test/platinum/inference/100_semantic_text_auto_prefiltering.yml",
  "name": "Test bool filter with should nested and non-nested terms - min_should_match is 2",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "query": {
          "bool": {
            "filter": [
              {
                "bool": {
                  "minimum_should_match": 2,
                  "should": [
                    {
                      "term": {
                        "keyword_field_1": {
                          "value": "label_1"
                        }
                      }
                    },
                    {
                      "nested": {
                        "path": "nested_field",
                        "query": {
                          "term": {
                            "nested_field.keyword_field_sub": {
                              "value": "label_1"
                            }
                          }
                        }
                      }
                    }
                  ]
                }
              }
            ],
            "must": [
              {
                "match": {
                  "dense_field_1": {
                    "query": "violins"
                  }
                }
              }
            ]
          }
        }
      },
      "index": "test-index",
      "typed_keys": true
    }
  },
  "response": {
    "headers": {
      "content-length": "239",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "error": {
        "reason": "Inference endpoint not found [dense-inference-id]",
        "root_cause": [
          {
            "reason": "Inference endpoint not found [dense-inference-id]",
            "type": "resource_not_found_exception"
          }
        ],
        "type": "resource_not_found_exception"
      },
      "status": 404
    },
    "statusCode": 404
  }
}