{
  "api": "search",
  "file": "/test/free/search.vectors/110_knn_query_with_filter.yml",
  "name": "POST_FILTER: knn query with filter from a parent bool query as post-filter",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "fields": [
          "my_name"
        ],
        "query": {
          "bool": {
            "must": [
              {
                "term": {
                  "my_name": "v2"
                }
              },
              {
                "knn": {
                  "field": "my_vector",
                  "filter": {
                    "term": {
                      "my_name": "v1"
                    }
                  },
                  "num_candidates": 5,
                  "query_vector": [
                    1,
                    1,
                    1,
                    1
                  ]
                }
              }
            ]
          }
        },
        "size": 3
      },
      "index": "my_index",
      "typed_keys": true
    }
  },
  "response": {
    "headers": {
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "_shards": {
        "failed": 0,
        "skipped": 0,
        "successful": 1,
        "total": 1
      },
      "hits": {
        "hits": [
        ],
        "max_score": null,
        "total": {
          "relation": "eq",
          "value": 0
        }
      },
      "timed_out": false,
      "took": 3
    },
    "statusCode": 200
  }
}