{
  "api": "search",
  "file": "/test/platinum/inference/90_semantic_text_highlighter.yml",
  "name": "Highlighting with knn with similarity",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "highlight": {
          "fields": {
            "body": {
              "number_of_fragments": 3,
              "type": "semantic"
            }
          }
        },
        "query": {
          "knn": {
            "field": "body",
            "k": 10,
            "num_candidates": 10,
            "query_vector_builder": {
              "text_embedding": {
                "model_text": "What is Elasticsearch?"
              }
            },
            "similarity": 0.9977
          }
        }
      },
      "index": "test-dense-index",
      "typed_keys": true
    }
  },
  "response": {
    "headers": {
      "content-length": "271",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "error": {
        "reason": "[dense-inference-id] is not an inference service model or a deployed ml model",
        "root_cause": [
          {
            "reason": "[dense-inference-id] is not an inference service model or a deployed ml model",
            "type": "status_exception"
          }
        ],
        "type": "status_exception"
      },
      "status": 404
    },
    "statusCode": 404
  }
}