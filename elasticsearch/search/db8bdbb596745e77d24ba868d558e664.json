        "failed": 0,
        "skipped": 0,
        "successful": 1,
        "total": 1
      },
      "hits": {
        "hits": [
          {
            "_id": "2",
            "_index": "nested_text",
            "_score": 0.9743162,
            "_source": {
              "paragraphs": [
                {
                  "text": "some text"
                },
                {
                  "other_nested_thing": [
