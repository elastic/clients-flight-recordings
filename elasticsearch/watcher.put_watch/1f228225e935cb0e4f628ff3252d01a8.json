{
  "api": "watcher.put_watch",
  "file": "/test/platinum/painless/10_basic.yml",
  "name": "Test executing logging action using scripts with params",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "actions": {
          "log": {
            "logging": {
              "text": {
                "id": "log-action",
                "params": {
                  "color": "yellow"
                }
              }
            }
          }
        },
        "condition": {
          "always": {
          }
        },
        "input": {
          "simple": {
          }
        },
        "trigger": {
          "schedule": {
            "interval": "1m"
          }
        }
      },
      "id": "test_watch"
    }
  },
  "response": {
    "headers": {
      "content-length": "80",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "_id": "test_watch",
      "_primary_term": 1,
      "_seq_no": 12,
      "_version": 9,
      "created": false
    },
    "statusCode": 200
  }
}