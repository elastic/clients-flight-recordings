{
  "api": "watcher.put_watch",
  "file": "/test/platinum/painless/20_minimal_body.yml",
  "name": "Test execute watch api with minimal body",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "actions": {
          "logging": {
            "logging": {
              "text": "foobar"
            }
          }
        },
        "condition": {
          "script": {
            "lang": "painless",
            "source": "ctx.payload.count == 1"
          }
        },
        "input": {
          "simple": {
            "count": 1
          }
        },
        "trigger": {
          "schedule": {
            "cron": "0 0 0 1 * ? 2099"
          }
        }
      },
      "id": "my_logging_watch"
    }
  },
  "response": {
    "headers": {
      "content-length": "85",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "_id": "my_logging_watch",
      "_primary_term": 1,
      "_seq_no": 15,
      "_version": 1,
      "created": true
    },
    "statusCode": 201
  }
}