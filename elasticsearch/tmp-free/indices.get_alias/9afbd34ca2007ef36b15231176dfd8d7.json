{
  "api": "indices.get_alias",
  "file": "/test/free/indices.get_alias/30_wildcards.yml",
  "name": "Wildcard exclusions does not trigger 404",
  "origin": "yaml",
  "request": {
    "args": {
      "name": [
        "-non-existent",
        "-non-existent*",
        "-another"
      ]
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "80",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "error": "alias [-non-existent] missing",
      "status": 404
    },
    "statusCode": 404
  }
}