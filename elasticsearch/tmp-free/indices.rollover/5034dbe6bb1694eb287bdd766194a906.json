      "conditions": {
        "[max_docs: 2]": false
      },
      "dry_run": false,
      "lazy": false,
      "new_index": "logs-000002",
      "old_index": "logs-1",
      "rolled_over": false,
      "shards_acknowledged": false
    },
    "statusCode": 200
  }
