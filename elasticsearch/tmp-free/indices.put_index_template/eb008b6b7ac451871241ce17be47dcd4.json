{
  "api": "indices.put_index_template",
  "file": "/test/free/index/91_metrics_no_subobjects.yml",
  "name": "Metrics object indexing with synthetic source",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "index_patterns": "test-*",
        "template": {
          "mappings": {
            "dynamic_templates": [
              {
                "no_subobjects": {
                  "mapping": {
                    "properties": {
                      "host.name": {
                        "type": "keyword"
                      }
                    },
                    "subobjects": false,
                    "type": "object"
                  },
                  "match": "metrics"
                }
              }
            ]
          },
          "settings": {
            "index": {
              "mapping": {
                "source": {
                  "mode": "synthetic"
                }
              }
            }
          }
        }
      },
      "name": "test"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "47",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true
    },
    "statusCode": 200
  }
}