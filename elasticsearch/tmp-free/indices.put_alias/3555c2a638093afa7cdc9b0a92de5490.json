{
  "api": "indices.put_alias",
  "file": "/test/free/aggregations/global_with_aliases.yml",
  "name": "global agg with a terms filtered alias",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "filter": {
          "terms": {
            "name": [
              "one"
            ]
          }
        }
      },
      "index": "test",
      "name": "test-filtered"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "62",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "errors": false
    },
    "statusCode": 200
  }
}