{
  "api": "cat.indices",
  "file": "/test/free/cat.indices/10_basic.yml",
  "name": "Test cat indices sort",
  "origin": "yaml",
  "request": {
    "args": {
      "format": "json",
      "h": [
        "status",
        "index"
      ],
      "index": [
        "b*",
        "foo"
      ],
      "s": [
        "status",
        "index:desc"
      ]
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": [
      {
        "index": "bar",
        "status": "close"
      },
      {
        "index": "foo",
        "status": "open"
      },
      {
        "index": "baz",
        "status": "open"
      }
    ],
    "statusCode": 200
  }
}