{
  "api": "create",
  "file": "/test/free/create/60_refresh.yml",
  "name": "When refresh url parameter is an empty string that means \"refresh immediately\"",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "foo": "bar"
      },
      "id": "1",
      "index": "test_1",
      "refresh": true
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "144",
      "content-type": "application/json",
      "location": "/test_1/_doc/1",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "_id": "1",
      "_index": "test_1",
      "_primary_term": 1,
      "_seq_no": 0,
      "_shards": {
        "failed": 0,
        "successful": 1,
        "total": 2
      },
      "_version": 1,
      "forced_refresh": true,
      "result": "created"
    },
    "statusCode": 201
  }
}