{
  "api": "indices.create",
  "file": "/test/free/logsdb/10_settings.yml",
  "name": "override sort field using nested field type",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "properties": {
            "@timestamp": {
              "type": "date"
            },
            "agent_id": {
              "type": "keyword"
            },
            "host.name": {
              "type": "keyword"
            },
            "http_method": {
              "type": "keyword"
            },
            "message": {
              "type": "text"
            },
            "nested": {
              "properties": {
                "keywords": {
                  "type": "keyword"
                }
              },
              "type": "nested"
            },
            "process_id": {
              "type": "integer"
            }
          }
        },
        "settings": {
          "index": {
            "mode": "logsdb",
            "number_of_replicas": 0,
            "number_of_shards": 2
          }
        }
      },
      "index": "test-nested"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "76",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "test-nested",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}