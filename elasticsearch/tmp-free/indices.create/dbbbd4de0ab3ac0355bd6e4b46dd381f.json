{
  "api": "indices.create",
  "file": "/test/free/cluster.stats/20_analysis_stats_synonyms.yml",
  "name": "get cluster stats returns synonym sets stats",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "settings": {
          "analysis": {
            "filter": {
              "a_repeated_synonyms_set_filter": {
                "synonyms_set": "reused-synonym-set",
                "type": "synonym"
              },
              "en-stem-filter": {
                "language": "light_english",
                "name": "light_english",
                "type": "stemmer"
              },
              "other_synonyms_filter": {
                "synonyms_set": "another-synonym-set",
                "type": "synonym"
              },
              "repeated_inline_filter": {
                "synonyms": [
                  "foo bar",
                  "bar => baz"
                ],
                "type": "synonym"
              }
            }
          }
        }
      },
      "index": "test-index2"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "75",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "test-index2",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}