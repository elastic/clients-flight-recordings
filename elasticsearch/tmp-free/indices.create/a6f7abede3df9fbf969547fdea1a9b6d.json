            }
          }
        },
        "settings": {
          "index": {
            "mapping": {
              "source": {
                "mode": "synthetic"
              }
            }
          }
        }
      },
      "index": "test"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "71",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "test",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
