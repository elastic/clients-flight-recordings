{
  "api": "indices.create",
  "file": "/test/free/aggregations/date_histogram.yml",
  "name": "Date histogram on two indices with same field but different date type",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "properties": {
            "date_field": {
              "type": "date"
            }
          }
        }
      },
      "index": "date_field_type_date"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "85",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "date_field_type_date",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}