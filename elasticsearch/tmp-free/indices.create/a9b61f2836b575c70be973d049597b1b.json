{
  "api": "indices.create",
  "file": "/test/free/indices/validate_query/10_synonyms.yml",
  "name": "validate query with synonyms",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "properties": {
            "field": {
              "analyzer": "syns",
              "type": "text"
            }
          }
        },
        "settings": {
          "index": {
            "analysis": {
              "analyzer": {
                "syns": {
                  "filter": [
                    "syns"
                  ],
                  "tokenizer": "standard"
                }
              },
              "filter": {
                "syns": {
                  "synonyms": [
                    "one,two"
                  ],
                  "type": "synonym"
                }
              }
            }
          }
        }
      },
      "index": "test"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "71",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "test",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}