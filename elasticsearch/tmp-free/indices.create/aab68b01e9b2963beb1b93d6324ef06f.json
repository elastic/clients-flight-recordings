{
  "api": "indices.create",
  "file": "/test/free/get/100_synthetic_source.yml",
  "name": "flattened field with ignore_above and arrays",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "_source": {
            "mode": "synthetic"
          },
          "properties": {
            "field": {
              "ignore_above": 10,
              "type": "flattened"
            }
          }
        }
      },
      "index": "test"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "71",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "test",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}