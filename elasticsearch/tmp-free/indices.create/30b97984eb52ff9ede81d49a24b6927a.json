{
  "api": "indices.create",
  "file": "/test/free/cluster.stats/20_analysis_stats_synonyms.yml",
  "name": "get cluster stats returns synonym sets stats",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "settings": {
          "analysis": {
            "filter": {
              "another_synonyms_path_filter": {
                "synonyms_path": "/another/different/path",
                "type": "synonym_graph"
              },
              "bigram_max_size": {
                "max": 16,
                "min": 0,
                "type": "length"
              },
              "other_inline_filter": {
                "synonyms": [
                  "foo bar baz"
                ],
                "type": "synonym"
              },
              "other_synonyms_path_filter": {
                "synonyms_path": "/a/different/path",
                "type": "synonym_graph"
              },
              "synonyms_inline_filter": {
                "synonyms": [
                  "foo bar",
                  "bar => baz"
                ],
                "type": "synonym"
              },
              "synonyms_path_filter": {
                "synonyms_path": "/a/reused/path",
                "type": "synonym"
              },
              "synonyms_set_filter": {
                "synonyms_set": "reused-synonym-set",
                "type": "synonym_graph"
              }
            }
          }
        }
      },
      "index": "test-index1"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "75",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "test-index1",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}