{
  "api": "indices.create",
  "file": "/test/free/tsdb/160_nested_fields.yml",
  "name": "Create TSDB index with field of nested type",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "properties": {
            "@timestamp": {
              "type": "date"
            },
            "courses": {
              "properties": {
                "credits": {
                  "type": "integer"
                },
                "name": {
                  "type": "keyword"
                }
              },
              "type": "nested"
            },
            "department": {
              "time_series_dimension": true,
              "type": "keyword"
            },
            "staff": {
              "type": "integer"
            }
          }
        },
        "settings": {
          "index": {
            "mode": "time_series",
            "number_of_replicas": 1,
            "number_of_shards": 1,
            "routing_path": [
              "department"
            ],
            "time_series": {
              "end_time": "2021-04-29T00:00:00Z",
              "start_time": "2021-04-28T00:00:00Z"
            }
          }
        }
      },
      "index": "test"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "71",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "test",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}