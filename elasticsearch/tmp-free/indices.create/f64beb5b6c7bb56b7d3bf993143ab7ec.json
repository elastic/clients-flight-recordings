{
  "api": "indices.create",
  "file": "/test/free/indices.analyze/15_analyze.yml",
  "name": "Synonym filter with tokenizer",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "settings": {
          "index": {
            "analysis": {
              "filter": {
                "synonym": {
                  "synonyms": [
                    "kimchy => shay"
                  ],
                  "type": "synonym"
                }
              },
              "tokenizer": {
                "trigram": {
                  "max_gram": 3,
                  "min_gram": 3,
                  "type": "ngram"
                }
              }
            }
          }
        }
      },
      "index": "test_synonym"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "79",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "test_synonym",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}