{
  "api": "delete",
  "file": "/test/free/delete/50_refresh.yml",
  "name": "When refresh url parameter is an empty string that means \"refresh immediately\"",
  "origin": "yaml",
  "request": {
    "args": {
      "id": "1",
      "index": "test_1",
      "refresh": true
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "142",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "_id": "1",
      "_index": "test_1",
      "_primary_term": 1,
      "_seq_no": 1,
      "_shards": {
        "failed": 0,
        "successful": 1,
        "total": 2
      },
      "_version": 2,
      "forced_refresh": true,
      "result": "deleted"
    },
    "statusCode": 200
  }
}