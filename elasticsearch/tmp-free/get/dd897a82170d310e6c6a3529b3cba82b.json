  "request": {
    "args": {
      "id": "1",
      "index": "test",
      "stored_fields": [
        "_ignored"
      ]
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "98",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "_id": "1",
      "_index": "test",
      "_primary_term": 1,
      "_seq_no": 0,
      "_version": 1,
      "found": true
    },
    "statusCode": 200
  }
}