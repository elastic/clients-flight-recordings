{
  "api": "get",
  "file": "/test/free/indices.clone/10_basic.yml",
  "name": "Clone index via API",
  "origin": "yaml",
  "request": {
    "args": {
      "id": "1",
      "index": "target"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-length": "119",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "_id": "1",
      "_index": "target",
      "_primary_term": 1,
      "_seq_no": 0,
      "_source": {
        "foo": "hello world"
      },
      "_version": 1,
      "found": true
    },
    "statusCode": 200
  }
}