{
  "api": "search",
  "file": "/test/free/search.vectors/130_knn_query_nested_search.yml",
  "name": "nested kNN search post-filtered on top level fields",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "fields": [
          "name"
        ],
        "query": {
          "bool": {
            "must": [
              {
                "term": {
                  "name": "rabbit.jpg"
                }
              },
              {
                "nested": {
                  "path": "nested",
                  "query": {
                    "knn": {
                      "field": "nested.vector",
                      "num_candidates": 1,
                      "query_vector": [
                        -0.5,
                        90,
                        -10,
                        14.8,
                        -156
                      ]
                    }
                  }
                }
              }
            ]
          }
        }
      },
      "index": "test",
      "typed_keys": true
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "_shards": {
        "failed": 0,
        "skipped": 0,
        "successful": 1,
        "total": 1
      },
      "hits": {
        "hits": [
        ],
        "max_score": null,
        "total": {
          "relation": "eq",
          "value": 0
        }
      },
      "timed_out": false,
      "took": 1
    },
    "statusCode": 200
  }
}