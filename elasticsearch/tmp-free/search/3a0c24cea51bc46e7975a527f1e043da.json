{
  "api": "search",
  "file": "/test/free/search.vectors/120_knn_query_multiple_shards.yml",
  "name": "Aggregations with collected number of docs depends on num_candidates",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "aggs": {
          "my_agg2": {
            "terms": {
              "field": "my_name",
              "order": {
                "_key": "asc"
              }
            }
          },
          "my_sum_buckets": {
            "sum_bucket": {
              "buckets_path": "my_agg2>_count"
            }
          }
        },
        "query": {
          "knn": {
            "field": "my_vector",
            "num_candidates": 3,
            "query_vector": [
              1,
              1,
              1,
              1
            ]
          }
        },
        "size": 2
      },
      "index": "my_index",
      "typed_keys": true
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "_shards": {
        "failed": 0,
        "skipped": 0,
        "successful": 2,
        "total": 2
      },
      "aggregations": {
        "simple_value#my_sum_buckets": {
          "value": 6
        },
        "sterms#my_agg2": {
          "buckets": [
            {
              "doc_count": 2,
              "key": "v1"
            },
            {
              "doc_count": 4,
              "key": "v2"
            }
          ],
          "doc_count_error_upper_bound": 0,
          "sum_other_doc_count": 0
        }
      },
      "hits": {
        "hits": [
          {
            "_id": "1",
            "_index": "my_index",
            "_score": 1,
            "_source": {
              "my_name": "v1",
              "my_vector": [
                1,
                1,
                1,
                1
              ]
            }
          },
          {
            "_id": "2",
            "_index": "my_index",
            "_score": 0.5,
            "_source": {
              "my_name": "v2",
              "my_vector": [
                1,
                1,
                1,
                2
              ]
            }
          }
        ],
        "max_score": 1,
        "total": {
          "relation": "eq",
          "value": 6
        }
      },
      "timed_out": false,
      "took": 4
    },
    "statusCode": 200
  }
}