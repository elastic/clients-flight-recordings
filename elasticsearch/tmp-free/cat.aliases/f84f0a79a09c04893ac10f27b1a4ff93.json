{
  "api": "cat.aliases",
  "file": "/test/free/cat.aliases/20_headers.yml",
  "name": "Simple alias with yaml body through Accept header",
  "origin": "yaml",
  "request": {
    "args": {
      "format": "json"
    },
    "headers": {
      "Accept": "application/yaml"
    }
  },
  "response": {
    "headers": {
      "content-encoding": "gzip",
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": [
      {
        "alias": "test_alias",
        "filter": "-",
        "index": "test",
        "is_write_index": "-",
        "routing.index": "-",
        "routing.search": "-"
      }
    ],
    "statusCode": 200
  }
}