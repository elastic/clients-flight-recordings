{
  "api": "indices.put_index_template",
  "file": "/test/platinum/exponential_histogram/50_time_series_metric.yml",
  "name": "Merging templates with time series metric",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "composed_of": [
          "metric-histogram"
        ],
        "index_patterns": [
          "my-templated-index"
        ],
        "template": {
          "mappings": {
            "properties": {
              "my_counter": {
                "time_series_metric": "counter",
                "type": "long"
              }
            }
          }
        }
      },
      "name": "only-histogram"
    }
  },
  "response": {
    "headers": {
      "content-length": "21",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true
    },
    "statusCode": 200
  }
}