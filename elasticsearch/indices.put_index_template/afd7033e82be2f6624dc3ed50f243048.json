{
  "api": "indices.put_index_template",
  "file": "/test/free/indices.put_index_template/10_basic.yml",
  "name": "Add data stream lifecycle with downsampling",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "data_stream": {
        },
        "index_patterns": "downsampling-*",
        "template": {
          "lifecycle": {
            "data_retention": "10d",
            "downsampling": [
              {
                "after": "1d",
                "fixed_interval": "5m"
              }
            ],
            "downsampling_method": "last_value"
          }
        }
      },
      "name": "test-lifecycle"
    }
  },
  "response": {
    "headers": {
      "content-length": "21",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true
    },
    "statusCode": 200
  }
}