{
  "api": "indices.put_index_template",
  "file": "/test/free/index/92_metrics_auto_subobjects.yml",
  "name": "Metrics object indexing",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "index_patterns": "test-*",
        "template": {
          "mappings": {
            "dynamic_templates": [
              {
                "no_subobjects": {
                  "mapping": {
                    "properties": {
                      "host.name": {
                        "type": "keyword"
                      }
                    },
                    "subobjects": "auto",
                    "type": "object"
                  },
                  "match": "metrics"
                }
              }
            ]
          }
        }
      },
      "name": "test"
    }
  },
  "response": {
    "headers": {
      "content-length": "21",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true
    },
    "statusCode": 200
  }
}