    "args": {
      "body": {
        "template": {
          "mappings": {
            "properties": {
              "my_histogram": {
                "time_series_metric": "histogram",
                "type": "exponential_histogram"
              }
            }
          }
        }
      },
      "name": "metric-histogram"
    }
  },
  "response": {
    "headers": {
      "content-length": "21",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true
    },
    "statusCode": 200
  }
}