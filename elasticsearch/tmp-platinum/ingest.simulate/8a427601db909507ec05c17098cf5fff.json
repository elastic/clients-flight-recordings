{
  "api": "ingest.simulate",
  "file": "/test/platinum/redact/10_redact_processor.yml",
  "name": "Test redact",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "docs": [
          {
            "_source": {
              "to_redact": "this-email@address.com will be redacted"
            }
          }
        ],
        "pipeline": {
          "processors": [
            {
              "redact": {
                "field": "to_redact",
                "patterns": [
                  "%{EMAILADDRESS:EMAIL}",
                  "%{IP:IP_ADDRESS}"
                ]
              }
            }
          ]
        }
      }
    }
  },
  "response": {
    "headers": {
      "content-length": "173",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "docs": [
        {
          "doc": {
            "_id": "_id",
            "_index": "_index",
            "_ingest": {
              "timestamp": "2025-05-24T10:33:33.36919613Z"
            },
            "_source": {
              "to_redact": "<EMAIL> will be redacted"
            },
            "_version": "-3"
          }
        }
      ]
    },
    "statusCode": 200
  }
}