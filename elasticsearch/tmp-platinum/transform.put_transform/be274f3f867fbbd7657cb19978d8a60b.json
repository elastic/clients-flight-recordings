{
  "api": "transform.put_transform",
  "file": "/custom-test/platinum/transform/reset_transform.yml",
  "name": "Test put transform to reset",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "dest": {
          "index": "airline-dest"
        },
        "frequency": "5s",
        "pivot": {
          "aggs": {
            "avg_response": {
              "avg": {
                "field": "responsetime"
              }
            }
          },
          "group_by": {
            "airline": {
              "terms": {
                "field": "airline"
              }
            }
          }
        },
        "source": {
          "index": "airline-data"
        }
      },
      "transform_id": "frequency-too-low"
    }
  },
  "response": {
    "headers": {
      "content-length": "21",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true
    },
    "statusCode": 200
  }
}