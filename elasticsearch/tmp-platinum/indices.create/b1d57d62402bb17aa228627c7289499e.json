{
  "api": "indices.create",
  "file": "/test/platinum/analytics/histogram.yml",
  "name": "histogram with synthetic source and ignore_malformed",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "_source": {
            "mode": "synthetic"
          },
          "properties": {
            "latency": {
              "ignore_malformed": true,
              "type": "histogram"
            }
          }
        }
      },
      "index": "histo_synthetic"
    }
  },
  "response": {
    "headers": {
      "content-length": "74",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "histo_synthetic",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}