{
  "api": "rollup.put_job",
  "file": "/test/platinum/rollup/rollup_search.yml",
  "name": "Wildcards matching two rollup indices",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "cron": "*/30 * * * * ?",
        "groups": {
          "date_histogram": {
            "calendar_interval": "1h",
            "field": "timestamp"
          },
          "terms": {
            "fields": [
              "partition"
            ]
          }
        },
        "index_pattern": "bar",
        "metrics": [
          {
            "field": "price",
            "metrics": [
              "max"
            ]
          }
        ],
        "page_size": 10,
        "rollup_index": "bar_rollup"
      },
      "id": "bar"
    },
