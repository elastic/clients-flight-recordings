{
  "api": "search",
  "file": "/test/platinum/wildcard/10_wildcard_basic.yml",
  "name": "Scripted Aggs work",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "aggs": {
          "top_vals": {
            "terms": {
              "script": "doc['my_wildcard']"
            }
          }
        },
        "query": {
          "wildcard": {
            "my_wildcard": {
              "value": "*goodbye*"
            }
          }
        },
        "track_total_hits": true
      },
      "typed_keys": true
    }
  },
  "response": {
    "headers": {
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "_shards": {
        "failed": 0,
        "skipped": 0,
        "successful": 1,
        "total": 1
      },
      "aggregations": {
        "sterms#top_vals": {
          "buckets": [
            {
              "doc_count": 1,
              "key": "goodbye world"
            }
          ],
          "doc_count_error_upper_bound": 0,
          "sum_other_doc_count": 0
        }
      },
      "hits": {
        "hits": [
          {
            "_id": "2",
            "_index": "test-index",
            "_score": 1,
            "_source": {
              "my_wildcard": "goodbye world"
            }
          }
        ],
        "max_score": 1,
        "total": {
          "relation": "eq",
          "value": 1
        }
      },
      "timed_out": false,
      "took": 17
    },
    "statusCode": 200
  }
}