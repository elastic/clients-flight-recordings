{
  "api": "search",
  "file": "/test/platinum/analytics/boxplot.yml",
  "name": "Search With Runtime Field",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "aggs": {
          "plot": {
            "boxplot": {
              "field": "load_time_adjusted"
            }
          }
        },
        "query": {
          "exists": {
            "field": "load_time"
          }
        },
        "runtime_mappings": {
          "load_time_adjusted": {
            "script": {
              "params": {
                "multiplier": 10
              },
              "source": "emit((long)((doc['load_time'].value == 10 ? 1000 : doc['load_time'].value) * params.multiplier))"
            },
            "type": "long"
          }
        },
        "size": 0
      },
      "index": "latency",
      "typed_keys": true
    }
  },
  "response": {
    "headers": {
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "_shards": {
        "failed": 0,
        "skipped": 0,
        "successful": 1,
        "total": 1
      },
      "aggregations": {
        "boxplot#plot": {
          "lower": 20,
          "max": 10000,
          "min": 20,
          "q1": 30,
          "q2": 50,
          "q3": 150,
          "upper": 150
        }
      },
      "hits": {
        "hits": [
        ],
        "max_score": null,
        "total": {
          "relation": "eq",
          "value": 5
        }
      },
      "timed_out": false,
      "took": 97
    },
    "statusCode": 200
  }
}