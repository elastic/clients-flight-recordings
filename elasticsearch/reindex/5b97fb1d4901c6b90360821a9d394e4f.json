{
  "api": "reindex",
  "file": "/test/free/reindex/100_tsdb.yml",
  "name": "from tsdb to tsdb modifying timestamp",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "dest": {
          "index": "tsdb_new"
        },
        "script": {
          "source": "ctx._source[\"@timestamp\"] = ctx._source[\"@timestamp\"].replace(\"-04-\", \"-05-\")"
        },
        "source": {
          "index": "tsdb"
        }
      },
      "refresh": true
    }
  },
  "response": {
    "headers": {
      "content-length": "240",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "batches": 1,
      "created": 4,
      "deleted": 0,
      "failures": [
      ],
      "noops": 0,
      "requests_per_second": -1,
      "retries": {
        "bulk": 0,
        "search": 0
      },
      "throttled_millis": 0,
      "throttled_until_millis": 0,
      "timed_out": false,
      "took": 31,
      "total": 4,
      "updated": 0,
      "version_conflicts": 0
    },
    "statusCode": 200
  }
}