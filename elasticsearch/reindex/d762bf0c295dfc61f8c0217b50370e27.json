      "created": 1,
      "deleted": 1,
      "failures": [
      ],
      "noops": 0,
      "requests_per_second": -1,
      "retries": {
        "bulk": 0,
        "search": 0
      },
      "throttled_millis": 0,
      "throttled_until_millis": 0,
      "timed_out": false,
      "took": 17,
      "total": 3,
      "updated": 1,
      "version_conflicts": 0
    },
    "statusCode": 200
  }
}