{
  "api": "reindex",
  "file": "/test/free/reindex/85_scripting.yml",
  "name": "Reindex all docs with one doc deletion",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "dest": {
          "index": "index2"
        },
        "script": {
          "lang": "painless",
          "source": "ctx._id = ctx._source.lang + '_' + ctx._source.id; if (ctx._source.lang != \"en\" ) {ctx.op = 'delete'}"
        },
        "source": {
          "index": "index1"
        }
      },
      "refresh": true
    }
  },
  "response": {
    "headers": {
      "content-length": "240",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
