            "downsampling_method": "aggregate",
            "enabled": false
          },
          "name": "data-stream-with-lifecycle"
        },
        {
          "lifecycle": {
            "data_retention": "30d",
            "downsampling": [
              {
                "after": "10d",
                "fixed_interval": "1h"
              },
              {
                "after": "100d",
                "fixed_interval": "10h"
              }
            ],
            "downsampling_method": "aggregate",
            "enabled": false
          },
          "name": "simple-data-stream1"
        }
      ],
      "global_retention": {
      }
