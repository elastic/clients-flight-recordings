{
  "api": "indices.put_data_stream_mappings",
  "file": "/test/free/indices.simulate_index_template/10_basic.yml",
  "name": "Simulate index template with data stream with mapping and setting overrides",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "properties": {
          "foo": {
            "type": "keyword"
          }
        }
      },
      "name": "test"
    }
  },
  "response": {
    "headers": {
      "content-type": "application/json",
      "transfer-encoding": "chunked",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "data_streams": [
        {
          "applied_to_data_stream": true,
          "effective_mappings": {
            "_data_stream_timestamp": {
              "enabled": true
            },
            "properties": {
              "@timestamp": {
                "type": "date"
              },
              "foo": {
                "type": "keyword"
              }
            }
          },
          "mappings": {
            "properties": {
              "foo": {
                "type": "keyword"
              }
            }
          },
          "name": "test"
        }
      ]
    },
    "statusCode": 200
  }
}