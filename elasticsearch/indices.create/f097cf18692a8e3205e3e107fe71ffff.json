{
  "api": "indices.create",
  "file": "/test/free/tsdb/60_add_dimensions.yml",
  "name": "add dimensions to some dims with dynamic_template over index",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "dynamic_templates": [
            {
              "keywords": {
                "mapping": {
                  "time_series_dimension": true,
                  "type": "keyword"
                },
                "match_mapping_type": "string"
              }
            }
          ],
          "properties": {
            "@timestamp": {
              "type": "date"
            },
            "metricset": {
              "time_series_dimension": true,
              "type": "keyword"
            }
          }
        },
        "settings": {
          "index": {
            "mode": "time_series",
            "routing_path": [
              "metricset"
            ],
            "time_series": {
              "end_time": "2021-04-29T00:00:00Z",
              "start_time": "2021-04-28T00:00:00Z"
            }
          }
        }
      },
      "index": "test"
    }
  },
  "response": {
    "headers": {
      "content-length": "63",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "test",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}