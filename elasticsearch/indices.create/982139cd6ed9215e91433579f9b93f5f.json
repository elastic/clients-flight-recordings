{
  "api": "indices.create",
  "file": "/test/free/indices.resolve_index/25_resolve_index_with_mode.yml",
  "name": "resolve index returns mode for standard and time_series indices",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "properties": {
            "@timestamp": {
              "type": "date"
            },
            "host": {
              "time_series_dimension": true,
              "type": "keyword"
            },
            "metric": {
              "type": "keyword"
            },
            "value": {
              "type": "double"
            }
          }
        },
        "settings": {
          "index": {
            "mode": "time_series",
            "routing_path": [
              "host"
            ]
          },
          "number_of_replicas": 0,
          "number_of_shards": 1
        }
      },
      "index": "my-ts-index"
    }
  },
  "response": {
    "headers": {
      "content-length": "70",
      "content-type": "application/json",
      "x-elastic-product": "Elasticsearch"
    },
    "payload": {
      "acknowledged": true,
      "index": "my-ts-index",
      "shards_acknowledged": true
    },
    "statusCode": 200
  }
}