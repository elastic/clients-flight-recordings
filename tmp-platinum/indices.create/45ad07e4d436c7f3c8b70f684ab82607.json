{
  "api": "indices.create",
  "file": "/test/platinum/esql/26_aggs_bucket.yml",
  "name": "Datetime interval as string",
  "origin": "yaml",
  "request": {
    "args": {
      "body": {
        "mappings": {
          "properties": {
            "ts": {
              "type": "date"
            }
          }
